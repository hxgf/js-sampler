{{#if is_admin}}

<div class="flex justify-between bg-subnav w-100 ph3 pv2 pv3-ns ph4-m ph5-l">
	<div class="f6 flex-grow">
		<a class="link black-50 dib mr4" href="/developments" title="All Developments">All Developments</a>
		<a class="link {{#unless data._id}}black-90{{else}}black-50{{/unless}} dib mr4" href="/developments/edit/new" title="New Development">+ New Development</a>
	</div>
</div>

<div class="ph3 pv4 ph5-l">
	<form class="admin-edit" action="javascript:void(0);">

		<div class="cf">

			<div class="fl w-100 w-50-ns pr3-ns">

				<div class="cf mb4">

					<div class="fl w-50 pr2">

						<div class="required">

							<label class="db mb2 roboto ttu b f6">Title <small class="dwa-red roboto b ml2 dib v-mid">*
									required</small></label>
							<input class="db w-100 pa2 f5" type="text" name="title" value="{{data.title}}" />

						</div>

					</div>
					<div class="fl w-50 pl2">

						<div class="required">
							<label class="db mb2 roboto ttu b f6">Short Code <small class="dwa-red roboto b ml2 dib v-mid">*
									required</small></label>
							<input class="db w-100 pa2 f5" type="text" name="shortcode" value="{{data.shortcode}}" />
						</div>

					</div>

				</div>

				<div class="cf mb4">

					<div class="fl w-50 pr2">

						<label class="db mb2 roboto ttu b f6">Domain Name</label>
						<input class="db w-100 pa2 f5" type="text" name="domain" value="{{data.domain}}" />

					</div>
					<div class="fl w-50 pl2">

						<label class="db mb2 roboto ttu b f6">Appfolio Group</label>
						<input class="db w-100 pa2 f5" type="text" name="appfolio_group" value="{{data.appfolio_group}}" />

					</div>

				</div>

				<div class="mb4">
					<label class="db mb2 roboto ttu b f6">Global Analytics HTML</label>
					<textarea class="db w-100 pa2 f5 mb3 h4 lh-copy" name="analytics_html">{{data.analytics_html}}</textarea>
				</div>

				<div class="cf mb4">

					<div class="fl w-50 pr2 required">

						<label class="db mb2 roboto ttu b f6">Latitude</label>
						<input class="db w-100 pa2 f5" type="text" name="latitude" value="{{data.latitude}}" />

					</div>
					<div class="fl w-50 pl2 required">

						<label class="db mb2 roboto ttu b f6">Longitude</label>
						<input class="db w-100 pa2 f5" type="text" name="longitude" value="{{data.longitude}}" />

					</div>

				</div>

				<div class="cf mb4">

					<div class="fl w-50 pr2">

						<label class="db mb2 roboto ttu b f6">Address 1</label>
						<input class="db w-100 pa2 f5" type="text" name="address_1" value="{{data.address_1}}" />

					</div>
					<div class="fl w-50 pl2">

						<label class="db mb2 roboto ttu b f6">Address 2</label>
						<input class="db w-100 pa2 f5" type="text" name="address_2" value="{{data.address_2}}" />

					</div>

				</div>

				<div class="cf mb4">

					<div class="fl w-50 pr2">

						<label class="db mb2 roboto ttu b f6">City</label>
						<input class="db w-100 pa2 f5" type="text" name="address_city" value="{{data.address_city}}" />

					</div>
					<div class="fl w-50 pl2">

						<label class="db mb2 roboto ttu b f6">Zip</label>
						<input class="db w-100 pa2 f5" type="text" name="address_zip" value="{{data.address_zip}}" />

					</div>

				</div>

			</div>

			<div class="fl w-100 w-50-ns pl3-ns cf">

				<div class="cf mb4">
					<div class="fl w-50 pr2">
						<label class="db mb2 roboto ttu b f6">Logo</label>
						<div class="roboto black-30 b mb2 fs80">
							* Min 500px
						</div>
						<div class="roboto black-30 b mb2 fs80">
							* JPG, PNG, GIF
						</div>
					</div>
					<div class="fl w-50">
						<!--[if lte IE 8]>
							<small class="db red mb3 lh-copy">* Your browser doesn't support photo uploads. Please upgrade your browser to the latest version of Internet Explorer. Firefox and Google Chrome are also supported.</small>
							<![endif]-->
						<div class="upload-container dz-2" onclick="void(0)">
							<div class="preview-image">
								{{#if data.logo_medium}}
								<img src="{{data.logo_medium}}" />
								<div class="upload-delete" data-id="2"><img src="/images/lib/uploader/delete.svg" /></div>
								{{/if}}
							</div>
							{{#unless data.logo_medium}}
							<div class="dropzone" id="upload_2" data-id="2"></div>
							{{/unless}}
						</div>
					</div>
				</div>

				<div class="mb4">
					<label class="db mb2 roboto ttu b f6">Tagline</label>
					<input class="db w-100 pa2 f5" type="text" name="tagline" value="{{data.tagline}}" />
				</div>

				<div class="cf">

					<div class="fl w-50 pr2">
						<label class="db mb2 roboto ttu b f6">Spotlight Image</label>

						<div class="roboto black-30 b mb2 fs80">
							* Min 500px
						</div>

						<div class="roboto black-30 b mb2 fs80">
							* JPG, PNG, GIF
						</div>

						<div class="roboto black-30 b mb2 fs80">
							* Visible on global about page
						</div>

					</div>

					<div class="fl w-50">
						<!--[if lte IE 8]>
						<small class="db red mb3 lh-copy">* Your browser doesn't support photo uploads. Please upgrade your browser to the latest version of Internet Explorer. Firefox and Google Chrome are also supported.</small>
						<![endif]-->

						<div class="upload-container dz-1 mb4" onclick="void(0)">
							<div class="preview-image">
								{{#if data.spotlight_image_medium}}
								<img src="{{data.spotlight_image_medium}}" />
								<div class="upload-delete" data-id="1"><img src="/images/lib/uploader/delete.svg" /></div>
								{{/if}}
							</div>
							{{#unless data.spotlight_image_medium}}
							<div class="dropzone" id="upload_1" data-id="1"></div>
							{{/unless}}
						</div>

					</div>

				</div>

				<div class="mb4">
					<label class="db mb2 "><span class="dib roboto ttu f6 b v-mid">Global Short Description </span><small
							class="ml2 roboto black-30 b dib v-mid">visible
							on global about page</small></label>
					<textarea class="db w-100 pa2 f5 mb3 h4 lh-copy"
						name="short_description">{{data.short_description}}</textarea>
				</div>

			</div>

		</div>

		<div class="w-100 bw3 bt b--black-05 mb4"></div>

		{{!-- // fixit demographics fields --}}

		<div style="margin-left: -.5rem; margin-right: -.5rem;">

			<div class="cf">

				<div class="fl w-100 w-50-m w-25-l pa2">

					<label class="db mb2 roboto ttu b f6">Total Leasable Footage</label>
